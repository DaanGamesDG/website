{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"jsx": "preserve",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"incremental": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"isolatedModules": true
	},
	"include": ["**/*.ts", "**/*.tsx"]
}
